                        <a href="#" class="main-menu__link">обувь</a>
                        <a href="#" class="main-menu__link">сумки</a>
                        <a href="#" class="main-menu__link">аксессуары</a>
                        <a href="#" class="main-menu__link">белье</a>
                        <a href="#" class="main-menu__link">красота</a>
                    </nav>
                </div>
            </div>
        </div>
    </main>
    <div class="main-footer">
        <!--@import "footer.html"-->
    </div>
    <script src="js/swiper.js"></script>
    <script src="js/jQuery.js"></script>
    <script src="js/main.js"></script>
</body>

</html>